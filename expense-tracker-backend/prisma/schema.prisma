// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
}

model Expense {
  id          Int      @id @default(autoincrement())
  title       String
  amount      Decimal
  category    String
  date        DateTime @default(now())
}